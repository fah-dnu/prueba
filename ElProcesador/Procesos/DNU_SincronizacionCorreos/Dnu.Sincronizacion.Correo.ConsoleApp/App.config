<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="BatchSincronizacionCorreo" type="System.Configuration.NameValueSectionHandler" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <connectionStrings>
    <add name="DataBaseProcesos" connectionString="data source=64.34.163.109;Initial Catalog=ProcesadorNocturno;User Id=sa;Password=dnu6996" providerName="System.Data.SqlClient" />
    <add name="BDAutorizadorRead" connectionString="Data Source=64.34.163.109;Initial Catalog=AUTORIZADOR_EFECTIVALE_TRAVEL;User ID=sa;Password=dnu6996"/>
    <add name="BDAutorizadorWritte" connectionString="Data Source=64.34.163.109;Initial Catalog=AUTORIZADOR_EFECTIVALE_TRAVEL;User ID=sa;Password=dnu6996"/>
  </connectionStrings>


  <log4net>

    <!-- Define some output appenders -->
    <appender name="EventosProcesador" type="log4net.Appender.RollingFileAppender">
      <file value="C:/ProcesadorNocturno/Logs/Procesador_Eventos.log" />
      <staticLogFileName value="true" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="_yyyyMMdd.lo\g" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%d{yyyy-MM-dd HH:mm:ss.fff}] [INFORMATIVO] %m%n" />
      </layout>
    </appender>

    <appender name="ErroresProcesador" type="log4net.Appender.RollingFileAppender">
      <file value="C:/ProcesadorNocturno/Logs/Procesador_Errores.log" />
      <staticLogFileName value="true" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="_yyyyMMdd.lo\g" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%d{yyyy-MM-dd HH:mm:ss.fff}] [ERROR] %m%n" />
      </layout>
    </appender>
    <appender name="FlujoProcesador" type="log4net.Appender.RollingFileAppender">
      <file value="C:/ProcesadorNocturno/Logs/Procesador_Flujo.log" />
      <staticLogFileName value="true" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="_yyyyMMdd.lo\g" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%d{yyyy-MM-dd HH:mm:ss.fff}] [ERROR] %m%n" />
      </layout>
    </appender>

    <appender name="EntradasCortador" type="log4net.Appender.RollingFileAppender">
      <file value="C:/ProcesadorNocturno/Logs/Eventos_EntradaSalida.log" />
      <staticLogFileName value="true" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="_yyyyMMdd.lo\g" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%d{yyyy-MM-dd HH:mm:ss.fff}] [WEBSERVICE] %m%n" />
      </layout>
    </appender>

    <appender name="EventosCortador" type="log4net.Appender.RollingFileAppender">
      <file value="C:/ProcesadorNocturno/Logs/Eventos_Eventos.log" />
      <staticLogFileName value="true" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="_yyyyMMdd.lo\g" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%d{yyyy-MM-dd HH:mm:ss.fff}] [INFORMATIVO] %m%n" />
      </layout>
    </appender>

    <appender name="ErroresCortador" type="log4net.Appender.RollingFileAppender">
      <file value="C:/ProcesadorNocturno/Logs/Eventos_Errores.log" />
      <staticLogFileName value="true" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="_yyyyMMdd.lo\g" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%d{yyyy-MM-dd HH:mm:ss.fff}] [ERROR] %m%n" />
      </layout>
    </appender>
    <!-- loggers -->

    <logger name="EventosProcesador">
      <appender-ref ref="EventosProcesador" />
    </logger>

    <logger name="ErroresProcesador">
      <appender-ref ref="ErroresProcesador" />
    </logger>
    <logger name="FlujosProcesador">
      <appender-ref ref="FlujoProcesador" />
    </logger>
    <logger name="Entradas">
      <appender-ref ref="EntradasCortador" />
    </logger>
    <logger name="Eventos">
      <appender-ref ref="EventosCortador" />
    </logger>

    <logger name="Errores">
      <appender-ref ref="ErroresCortador" />
    </logger>


  </log4net>
  
  
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-11.0.0.0" newVersion="11.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>